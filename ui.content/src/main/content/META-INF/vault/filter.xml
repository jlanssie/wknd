<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">
    <filter root="/conf/wknd" mode="merge"/>
    <filter root="/conf/wknd/settings/workflow" mode="update"/>
    <filter root="/content/wknd" mode="merge"/>
    <filter root="/content/dam/wknd" mode="merge">
        <exclude pattern="/content/dam/wknd(/.*)?"/>
        <include pattern="/content/dam/wknd/jcr:content(/.*)?"/>
    </filter>
    <filter root="/content/dam/wknd/asset.jpg" mode="merge"/>
    <filter root="/content/experience-fragments/wknd" mode="merge"/>
    <filter root="/var" mode="update"/>
</workspaceFilter>
